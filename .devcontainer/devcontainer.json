{
    "name": "Copilot Workshop",
    "image": "mcr.microsoft.com/devcontainers/python:3.11",
    "features": {
      "ghcr.io/devcontainers/features/python:1": {
        "version": "3.11"
      }
    },
    "postCreateCommand": "pip install pandas jupyter matplotlib seaborn",
    "customizations": {
      "vscode": {
        "extensions": [
          "GitHub.copilot",
          "ms-python.python",
          "ms-toolsai.jupyter"
        ]
      }
    }
  }
  